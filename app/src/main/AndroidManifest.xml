<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.boanergepro.pagomovil">

    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_mobile_svg2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.FormActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activities.ContactActivity" android:parentActivityName=".activities.FormActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".activities.FormActivity"/>
        </activity>
    </application>

</manifest>